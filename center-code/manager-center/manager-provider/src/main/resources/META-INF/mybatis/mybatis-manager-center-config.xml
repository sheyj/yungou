<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<!-- changes from the defaults -->
		<setting name="lazyLoadingEnabled" value="false" />
		<!-- JdbcType enumeration. Most common are: NULL, VARCHAR and OTHER -->
		<setting name="jdbcTypeForNull" value="NULL" />
		<setting name="defaultStatementTimeout" value="30" />
	</settings>
	
	<plugins>
        <plugin interceptor="com.yungou.o2o.tools.common.db.interceptor.PageInterceptor">
        </plugin>
	</plugins>
	<mappers>
		<mapper	resource="com/yungou/o2o/center/manager/provider/dal/database/MenuMapper.xml" />
		<mapper	resource="com/yungou/o2o/center/manager/provider/dal/database/RoleMapper.xml" />
		<mapper	resource="com/yungou/o2o/center/manager/provider/dal/database/RoleMenuMapper.xml" />
		<mapper	resource="com/yungou/o2o/center/manager/provider/dal/database/SystemDictMapper.xml" />
		<mapper	resource="com/yungou/o2o/center/manager/provider/dal/database/SystemParamMapper.xml" />
		<mapper	resource="com/yungou/o2o/center/manager/provider/dal/database/SystemUserMapper.xml" />
		<mapper	resource="com/yungou/o2o/center/manager/provider/dal/database/UserRoleMapper.xml" />
		<mapper	resource="com/yungou/o2o/center/manager/provider/dal/database/BrandMapper.xml" />
	</mappers>
	
	 
</configuration>